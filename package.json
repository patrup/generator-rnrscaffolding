{
  "name": "generator-rnrscaffolding",
  "version": "0.0.1",
  "description": "Scaffolding for React with Redux and Store",
  "homepage": "https://github.com/patrup/generator-rnrscaffolding#readme",
  "author": {
    "name": "Patrick Rupprecht",
    "email": "patrick.r@snafu.de",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "reactjs",
    "redux",
    "store",
    "scaffolding"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-xo": "^0.24.2",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^1.1.1",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.11.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^3.1.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrup/generator-rnrscaffolding.git"
  },
  "license": "ISC"
}
